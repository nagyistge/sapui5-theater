<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" 
		xmlns="http://java.sun.com/xml/ns/persistence" 
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
		xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="sapui5-theater-model-jpa" transaction-type="RESOURCE_LOCAL">
		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
		<class>com.sapui5theater.model.Genre</class>	
		<class>com.sapui5theater.model.Style</class>
		<class>com.sapui5theater.model.Mood</class>
		<class>com.sapui5theater.model.Theme</class>		
		<class>com.sapui5theater.model.Album</class>
		<class>com.sapui5theater.model.Artist</class>
		<class>com.sapui5theater.model.Track</class>
		<class>com.sapui5theater.model.AppUser</class>
		<properties>
			<property name="eclipselink.ddl-generation" value="drop-and-create-tables"/>
			<property name="eclipselink.jpql.parser" value="org.eclipse.persistence.queries.ANTLRQueryBuilder"/>
		</properties>
	</persistence-unit>
</persistence>
